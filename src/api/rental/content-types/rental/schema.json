{
  "kind": "collectionType",
  "collectionName": "rentals",
  "info": {
    "singularName": "rental",
    "pluralName": "rentals",
    "displayName": "Rental",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "rentalId": {
      "type": "uid"
    },
    "merchandise": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::merchandise.merchandise"
    },
    "options": {
      "type": "component",
      "repeatable": true,
      "component": "rentals.rent-rates"
    },
    "name": {
      "type": "string"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "rental"
    },
    "rating": {
      "type": "decimal",
      "min": 0,
      "max": 5,
      "default": 0
    },
    "totalReviews": {
      "type": "integer",
      "default": 0
    },
    "minPriceUSD": {
      "type": "decimal"
    },
    "minPriceLKR": {
      "type": "decimal"
    }
  }
}
