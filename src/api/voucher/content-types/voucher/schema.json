{
  "kind": "collectionType",
  "collectionName": "vouchers",
  "info": {
    "singularName": "voucher",
    "pluralName": "vouchers",
    "displayName": "Voucher",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "couponCode": {
      "type": "uid"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "CASH",
        "PERCENTAGE",
        "EXPERIENCE"
      ]
    },
    "expiryDate": {
      "type": "date"
    },
    "cash": {
      "type": "component",
      "component": "voucher.cash",
      "repeatable": false
    },
    "experience": {
      "type": "json"
    },
    "info": {
      "type": "json"
    },
    "voucherTemplate": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::voucher-template.voucher-template",
      "inversedBy": "vouchers"
    },
    "email": {
      "type": "string"
    },
    "percentageAmount": {
      "type": "decimal"
    },
    "voucherStatus": {
      "type": "enumeration",
      "enum": [
        "AVAILABLE",
        "CLAIMED",
        "EXPIRED",
        "UNPAID"
      ]
    },
    "reusable": {
      "type": "boolean",
      "required": true,
      "default": false
    }
  }
}
