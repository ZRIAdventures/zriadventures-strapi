{
  "kind": "collectionType",
  "collectionName": "experiences",
  "info": {
    "singularName": "experience",
    "pluralName": "experiences",
    "displayName": "Experience",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "experienceId": {
      "type": "uid"
    },
    "name": {
      "type": "string"
    },
    "location": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::location.location"
    },
    "about": {
      "type": "component",
      "component": "experience.about",
      "repeatable": false
    },
    "trending": {
      "type": "boolean"
    },
    "offer": {
      "type": "integer"
    },
    "additionalInformation": {
      "type": "component",
      "component": "experience.requirements",
      "repeatable": true
    },
    "included": {
      "type": "richtext"
    },
    "experience_categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::experience-category.experience-category"
    },
    "terms_and_condition": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::terms-and-condition.terms-and-condition"
    },
    "faqs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::faq.faq"
    },
    "extras": {
      "type": "component",
      "component": "experience.extras",
      "repeatable": true
    },
    "options": {
      "type": "component",
      "component": "experience.options",
      "repeatable": true
    },
    "bundle": {
      "type": "boolean",
      "default": false
    },
    "voucher_templates": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::voucher-template.voucher-template"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "difficulty": {
      "type": "enumeration",
      "enum": [
        "Easy",
        "Moderate",
        "Hard",
        "Challenging"
      ]
    }
  }
}
