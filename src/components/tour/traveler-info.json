{
  "collectionName": "components_tour_traveler_info",
  "info": {
    "displayName": "Traveler Info",
    "icon": "user",
    "description": "Individual traveler details"
  },
  "options": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "dateOfBirth": {
      "type": "date"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female",
        "Other",
        "Prefer not to say"
      ]
    },
    "ageCategory": {
      "type": "enumeration",
      "enum": [
        "Adult",
        "Child",
        "Infant"
      ],
      "default": "Adult"
    },
    "nationality": {
      "type": "string"
    },
    "passportNumber": {
      "type": "string"
    },
    "passportExpiry": {
      "type": "date"
    },
    "dietaryRequirements": {
      "type": "string"
    },
    "medicalConditions": {
      "type": "text"
    },
    "emergencyContactName": {
      "type": "string"
    },
    "emergencyContactPhone": {
      "type": "string"
    }
  }
}
